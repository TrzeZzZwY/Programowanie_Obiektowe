<UserControl x:Class="lab_9.Technology"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:lab_9"
             mc:Ignorable="d" Width="100" Height="25">
    <Grid>
        <Border x:Name="techBroder" CornerRadius="8"  Height="25" Margin="8,0,0,0">
            <TextBlock x:Name="techText" Padding="5" FontWeight="Bold"></TextBlock>
        </Border>
    </Grid>
</UserControl>
